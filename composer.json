{
    "name": "pre/plugin",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "yay/yay": "dev-master"
    },
    "autoload": {
        "files": [
            "src/autoload.php",
            "src/functions.php",
            "src/expanders.php",
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Pre\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Pre\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
