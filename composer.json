{
    "name": "pre/plugin",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "yay/yay": "dev-master#2a8be861eba653d50a20a9bb5a4d8f2391950a0c"
    },
    "autoload": {
        "files": [
            "src/autoload.php",
