{
    "name": "pre/plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "yay/yay": "^0.1.1",
        "friendsofphp/php-cs-fixer": "^2.0",
        "composer-plugin-api": "^1.1"
    },
    "autoload": {
        "files": [
            "src/autoload.php",
            "src/functions.php",
            "src/expanders.php",
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Pre\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "composer/composer": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Pre\\": "tests"
        }
    },
    "extra": {
        "class": [
            "Pre\\Plugin"
        ]
    }
}
